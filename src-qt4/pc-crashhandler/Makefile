crashhandler.o: crashhandler.cpp crashhandler.h \
		ui_crashhandler.h \
		details.h \
		ui_details.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o crashhandler.o crashhandler.cpp

details.o: details.cpp details.h \
		ui_details.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o details.o details.cpp

moc_crashhandler.o: moc_crashhandler.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_crashhandler.o moc_crashhandler.cpp

moc_details.o: moc_details.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_details.o moc_details.cpp

