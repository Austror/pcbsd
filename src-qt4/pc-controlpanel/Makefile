	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/applications/pccontrol.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/applications/ 


install_cleanitems: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/pc-controlpanel/items/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/pc-controlpanel/items/ 
	rm -rf /usr/local/share/pcbsd/pc-controlpanel/items


