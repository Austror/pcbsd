####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install_dotrans: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ 
	cd i18n && lrelease-qt4 -nounfinished *.ts && cp *.qm /usr/local/share/pcbsd/i18n/


install_icons: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/icons/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/icons/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/pc-softwaremanager/appcafe.png $(INSTALL_ROOT)/usr/local/share/pcbsd/icons/


uninstall_icons:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/pcbsd/icons/appcafe.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/icons/ 


install_desktop: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/applications/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/pc-softwaremanager/softmanager.desktop $(INSTALL_ROOT)/usr/local/share/applications/


uninstall_desktop:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/applications/softmanager.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/applications/ 


install_symlnk: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	ln -fs /usr/local/bin/pc-softwaremanager /usr/local/bin/appcafe


install:  install_target install_dotrans install_icons install_desktop install_symlnk  FORCE

uninstall: uninstall_target uninstall_icons uninstall_desktop   FORCE

FORCE:

