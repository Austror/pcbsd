####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/warden/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/warden/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/share/warden/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/share/warden/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/warden/bin/ 


install_doicon: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/warden/images/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/warden/images/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/warden-gui/warden.png $(INSTALL_ROOT)/usr/local/share/warden/images/


uninstall_doicon:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/warden/images/warden.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/warden/images/ 


install_dotrans: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/warden/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/warden/i18n/ 
	cd i18n && lrelease-qt4 -nounfinished *.ts && cp *.qm /usr/local/share/warden/i18n/


install_desktop: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/applications/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/warden-gui/warden.desktop $(INSTALL_ROOT)/usr/local/share/applications/


uninstall_desktop:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/applications/warden.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/applications/ 


install:  install_target install_doicon install_dotrans install_desktop  FORCE

uninstall: uninstall_target uninstall_doicon uninstall_desktop   FORCE

