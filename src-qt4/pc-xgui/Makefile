	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/pc-xgui/resources/nvidia-detect/nvidia9x-pciid.txt $(INSTALL_ROOT)/usr/local/share/pcbsd/xorg-gui/nvidia-detect/


uninstall_nvdetect:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/pcbsd/xorg-gui/nvidia-detect/detect-nvidia.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/pcbsd/xorg-gui/nvidia-detect/nvidia17-pciid.txt 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/pcbsd/xorg-gui/nvidia-detect/nvidia9x-pciid.txt
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/xorg-gui/nvidia-detect/ 


install_dotrans: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ 
	cd i18n && lrelease-qt4 -nounfinished *.ts && cp *.qm /usr/local/share/pcbsd/i18n/


install_doarch: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ 
	sh install-arch.sh `pwd`


install:  install_target install_bin install_scripts install_conf install_fluxbox install_carddetect install_settings install_temscripts install_temheader install_temdriver install_nvdetect install_dotrans install_doarch  FORCE

uninstall: uninstall_target uninstall_bin uninstall_scripts uninstall_conf uninstall_fluxbox uninstall_carddetect uninstall_settings uninstall_temscripts uninstall_temheader uninstall_temdriver uninstall_nvdetect   FORCE

FORCE:

