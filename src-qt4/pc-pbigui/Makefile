####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install_dotrans: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/i18n/ 
	cd i18n && lrelease-qt4 -nounfinished *.ts && cp *.qm /usr/local/share/pcbsd/i18n/


install_applnk: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/applications/ 
	cd resources && xdg-desktop-menu install pcbsd-pbi.desktop


install_pbiimg: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/pcbsd/images/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/pcbsd/images/ 
	cd resources && xdg-icon-resource install --context mimetypes --size 64 pcbsd-pbi.png application-pbi


install_mimexml: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/mime/packages/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/mime/packages/ 
	cd resources && xdg-mime install pcbsd-pbi.xml


install_runpbibin: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/pc-pbigui/resources/runpbi $(INSTALL_ROOT)/usr/local/bin/


uninstall_runpbibin:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/bin/runpbi
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install_runpbichmod: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	chmod 755 /usr/local/bin/runpbi


install:  install_target install_dotrans install_applnk install_pbiimg install_mimexml install_runpbibin install_runpbichmod  FORCE

uninstall: uninstall_target uninstall_runpbibin   FORCE

FORCE:

