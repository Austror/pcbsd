####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/bin/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/bin/ 


install_scripts: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/ 
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/setup-ssh-keys.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/check-backups.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/start-backup.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/functions.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/backup-rsync.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/restore-rsync.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/list-backup-rsync.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/
	-$(INSTALL_PROGRAM) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/scripts/kill-backup.sh $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/


uninstall_scripts:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/setup-ssh-keys.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/check-backups.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/start-backup.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/functions.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/backup-rsync.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/restore-rsync.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/list-backup-rsync.sh 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/kill-backup.sh
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/scripts/ 


install_conf: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/conf/life-preserver.conf $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/conf/rsync-excludes $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/conf/rsync-includes $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/


uninstall_conf:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/life-preserver.conf 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/rsync-excludes 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/rsync-includes
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/conf/ 


install_dotrans: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/lifePreserver/i18n/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/i18n/ 
	cd i18n && lrelease-qt4 -nounfinished *.ts && cp *.qm /usr/local/share/lifePreserver/i18n/


install_images: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/local/share/lifePreserver/images/ || $(MKDIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/images/ 
	-$(INSTALL_FILE) /usr/home/kris/source/pcbsd/current/src-qt4/life-preserver/images/lifepreserver.png $(INSTALL_ROOT)/usr/local/share/lifePreserver/images/


uninstall_images:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/local/share/lifePreserver/images/lifepreserver.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/local/share/lifePreserver/images/ 


install:  install_target install_scripts install_conf install_dotrans install_images  FORCE

uninstall: uninstall_target uninstall_scripts uninstall_conf uninstall_images   FORCE

